  "private": true,
  "workspaces": ["core", "types"],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "build:all": "npm run build:deps && npm run build",
    "build:deps": "npm run build:types && npm run build:core",
    "build:types": "npm run build -w types",
    "build:core": "npm run build -w core",
    "test": "jest",
    "test:single": "jest -t",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --format stylish",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --format stylish --fix",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
